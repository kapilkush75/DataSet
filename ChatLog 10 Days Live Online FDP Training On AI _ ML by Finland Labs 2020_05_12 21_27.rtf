import cv2\par
xml = r'C:\\Users\\umang\\Desktop\\face.xml'\par
image = r'C:\\Users\\umang\\Desktop\\smiles.jpg'\par
smile = r'C:\\Users\\umang\\Desktop\\smile.xml'\par
face_cascade = cv2.CascadeClassifier(xml)\par
smile_cascade = cv2.CascadeClassifier(smile)\par
img = cv2.imread(image)\par
faces = face_cascade.detectMultiScale(img,1.2,5)    ###(image name, scale factor, neighbors)\par
for x,y,w,h in faces:\par
    cv2.rectangle(img,(x,y),(x+w,y+h),(255,0,0),2)\par
    my_face = img[y:y+h, x:x+w]\par
    smiles = smile_cascade.detectMultiScale(my_face,1.8,12)\par
    for ex,ey,ew,eh in smiles:\par
        cv2.rectangle(my_face,(ex,ey),(ex+ew,ey+eh),(0,0,255),1)\par
cv2.imshow("Faces",img)\par
cv2.waitKey(0)\par
cv2.destroyAllWindows()\cf2\par
