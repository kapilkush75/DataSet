import numpy as np\par
import time\par
\par
imgpath1 =  r"E:\\Machine Learning - Umang\\OpenCv\\images\\lena_color_512.tif"\par
imgpath2 =  r"E:\\Machine Learning - Umang\\OpenCv\\images\\4.2.01.tiff"\par
    \par
img1 = cv2.imread(imgpath1, 1)\par
img2 = cv2.imread(imgpath2, 1)\par
    \par
for i in np.linspace(0, 1, 100):\par
    a = i\par
    b = 1 - a\par
    output = cv2.addWeighted(img1, a, img2, b, 0)\par
    cv2.imshow('Transition', output)\par
    time.sleep(0.1)    ## in secs\par
    if cv2.waitKey(1) == 27:   #esc key\par
        break\par
        \par
