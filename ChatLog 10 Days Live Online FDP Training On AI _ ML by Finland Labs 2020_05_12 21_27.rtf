while True:\par
    ret,frame = cam.read()\par
    hsv = cv2.cvtColor(frame,cv2.COLOR_BGR2HSV)\par
    ### Red colour\par
    low = np.array([140,150,0])\par
    high = np.array([180,255,255])\par
    image_mask = cv2.inRange(hsv,low,high)\par
    output = cv2.bitwise_and(frame,frame,mask=image_mask)\par
    cv2.imshow("HSV",hsv)  ### 13 is the ASCII of Enter\par
    cv2.imshow("MASK",image_mask)\par
    cv2.imshow("OUTPUT",output)\par
    if cv2.waitKey(1) == 13:\par
        break\par
